<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wx.mapper.UrlsMapper">

    <select id="list" resultType="com.wxs.entities.Urls">
        select * from urls;
    </select>

    <insert id="insert" parameterType="com.wxs.entities.Urls" >
        insert into urls (
            url, create_at
        ) values (
            #{url, jdbcType=VARCHAR},
            now()
        )
    </insert>
</mapper>